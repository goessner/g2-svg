{
  "name": "g2d-svg",
  "version": "0.3.5",
  "description": "g2-svg - SVG addon for g2",
  "keywords": [
    "vector",
    "graphics",
    "2d",
    "command queue",
    "canvas",
    "svg",
    "javascript"
  ],
  "main": "g2.svg.js",
  "scripts": {
    "build": "npm run minify && npm run cptest",
    "minify": "uglifyjs ./g2.svg.js -o ./g2.svg.min.js --comments -m",
    "cptest": "copyfiles -f ./test/*.* ../goessner.github.io/g2-svg/test"
  },
  "author": "Stefan Goessner <fang03@web.de>",
  "repository": {
    "type": "git",
    "url": "https://github.com/goessner/g2-svg.git"
  },
  "license": "MIT",
  "devDependencies": {
    "concat": "^1.0.0",
    "jsdoc-to-markdown": "^1.3.2",
    "jslint": "^0.9.5",
    "uglifyjs": "^2.4.10"
  }
}
